-- [[ DARK MASTER CYBER CONFIG ]] --
local CreatorName = "Dark Master"

local function ShowLoading()
    local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
    local MainFrame = Instance.new("Frame", ScreenGui)
    MainFrame.Size = UDim2.new(0, 400, 0, 150)
    MainFrame.Position = UDim2.new(0.5, -200, 0.5, -75)
    MainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 20) -- Navy Black
    MainFrame.BorderSizePixel = 2
    MainFrame.BorderColor3 = Color3.fromRGB(255, 0, 0) -- Red Border
    
    local BarFill = Instance.new("Frame", MainFrame)
    BarFill.Size = UDim2.new(0, 0, 0, 5)
    BarFill.Position = UDim2.new(0, 0, 1, -5)
    BarFill.BackgroundColor3 = Color3.fromRGB(0, 255, 255) -- Aqua Blue
    
    local Title = Instance.new("TextLabel", MainFrame)
    Title.Size = UDim2.new(1, 0, 1, 0)
    Title.Text = "DARK MASTER: ALL SEAS ENABLED"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.BackgroundTransparency = 1
    Title.TextSize = 20
    Title.Font = Enum.Font.GothamBold

    BarFill:TweenSize(UDim2.new(1, 0, 0, 5), "Out", "Quad", 2)
    task.wait(2)
    ScreenGui:Destroy()
end

ShowLoading()

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DARK MASTER HUB - V14", "Midnight") 

-- [ TABS ] --
local MainTab = Window:NewTab("Auto Farm")
local SeaTab = Window:NewTab("Sea Travel")
local VisualTab = Window:NewTab("Visuals & ESP")
local PlayerTab = Window:NewTab("Pro Player")

-- [ 1. WORLD TRAVEL (WORKS FOR ALL SEAS) ] --
local TravelSection = SeaTab:NewSection("Teleport to Worlds")

TravelSection:NewButton("Travel to Sea 1 (Old World)", "Requires Level 0+", function()
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelMain")
end)

TravelSection:NewButton("Travel to Sea 2 (New World)", "Requires Level 700+", function()
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")
end)

TravelSection:NewButton("Travel to Sea 3 (Third Sea)", "Requires Level 1500+", function()
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou")
end)

-- [ 2. FRUIT & PLAYER ESP ] --
local VisualSection = VisualTab:NewSection("Cyber ESP")

VisualSection:NewButton("Aqua Fruit ESP", "Glows Aqua Blue", function()
    for _, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Tool") or v:FindFirstChild("Handle") then
            local h = Instance.new("Highlight", v)
            h.FillColor = Color3.fromRGB(0, 255, 255)
        end
    end
end)

VisualSection:NewButton("Red Player ESP", "Glows Red", function()
    for _, p in pairs(game.Players:GetPlayers()) do
        if p ~= game.Players.LocalPlayer and p.Character then
            local h = Instance.new("Highlight", p.Character)
            h.FillColor = Color3.fromRGB(255, 0, 0)
        end
    end
end)

-- [ 3. AUTO FARM ] --
local FarmSection = MainTab:NewSection("Combat")
_G.AttackSpeed = 0.1
FarmSection:NewToggle("Auto Clicker (360Â°)", "Spin & Punch", function(state)
    _G.ProFarm = state
    while _G.ProFarm do
        game:GetService("VirtualUser"):CaptureController()
        game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0))
        game.Workspace.CurrentCamera.CFrame = game.Workspace.CurrentCamera.CFrame * CFrame.Angles(0, math.rad(5), 0)
        task.wait(_G.AttackSpeed)
    end
end)

-- [ 4. PRO MOVEMENT ] --
local MoveSection = PlayerTab:NewSection("Movement")
MoveSection:NewSlider("Cyber Speed", "Run Fast", 250, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MoveSection:NewButton("Infinite Jump", "Jump Air", function()
    game:GetService("UserInputService").JumpRequest:Connect(function()
        game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end)
end)
    _G.AutoStore = state
    while _G.AutoStore do
        for _, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") and v:FindFirstChild("Handle") then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StoreFruit", v.Name, v)
            end
        end
        task.wait(1)
    end
end)

-- [ 5. SEA TRAVEL ] --
local TravelSection = SeaTab:NewSection("World Warp")
TravelSection:NewButton("Sea 1", "Main", function() game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelMain") end)
TravelSection:NewButton("Sea 2", "Dressrosa", function() game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa") end)
TravelSection:NewButton("Sea 3", "Zou", function() game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou") end)
